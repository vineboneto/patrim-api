  "license": "MIT",
  "scripts": {
    "husky": "husky install .config/husky && husky add .config/husky/pre-commit \"yarn lint-staged && yarn test:staged\" && husky add .config/husky/pre-push \"yarn test:ci\" ",
    "husky:clean": "rimraf .config",
    "husky:install": "(yarn husky:clean) & yarn husky",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:staged": "yarn test - --findRelatedTests",
    "test:ci": "yarn test - --coverage"
  },
  "devDependencies": {
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.4.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
