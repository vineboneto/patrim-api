{
  "name": "patrim",
  "version": "0.1.1",
  "main": "index.js",
  "repository": "https://github.com/vineboneto/patrim-api.git",
  "author": "vineboneto <vineboneto@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "prepare": "npx prisma generate --schema=./src/infra/db/postgres-prisma/prisma/schema.prisma",
    "husky:clean": "rimraf .config",
    "husky:install": "husky install .config/husky",
    "husky:push": "husky add .config/husky/pre-push \"yarn test:ci\"",
    "husky:commit": "husky add .config/husky/pre-commit \"yarn lint-staged && yarn test:staged\"",
    "husky:commit-msg": "husky add .config/husky/commit-msg \".git/hooks/commit-msg $1 \"",
    "husky:config": "(yarn husky:commit) & (yarn husky:push) & (yarn husky:commit-msg)",
    "husky": "(yarn husky:clean) & (yarn husky:install) & yarn husky:config",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:watch": "yarn test - --watch",
    "test:staged": "yarn test - --findRelatedTests",
    "test:ci": "yarn test - --coverage",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info",
    "prisma:introspect": "prisma introspect --schema=./src/infra/db/postgres-prisma/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema=./src/infra/db/postgres-prisma/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema=./src/infra/db/postgres-prisma/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.7",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "faker": "^5.4.0",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prisma": "^2.19.0",
    "rimraf": "^3.0.2",
    "sucrase": "^3.17.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
